
  <edit-config>
    <target>
      <{{target}}/>
    </target>
     <default-operation>merge</default-operation>
    <error-option>rollback-on-error</error-option>
    <config>
      <interfaces xmlns="http://openconfig.net/yang/interfaces">
        <interface>
          <name>{{interface_name}}</name>
          <subinterfaces>
            <subinterface>
              <index>{{index}}</index>
              <config>
                <index>{{index}}</index>
              </config>
            </subinterface>
          </subinterfaces>
        </interface>
      </interfaces>
    </config>
  </edit-config>
