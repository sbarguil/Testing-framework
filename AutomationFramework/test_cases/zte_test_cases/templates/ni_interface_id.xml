 <edit-config>
    <target>
      <{{target}}/>
    </target>
     <default-operation>merge</default-operation>
    <error-option>rollback-on-error</error-option>
    <config>
      <network-instances xmlns="http://openconfig.net/yang/network-instance">
        <network-instance>
          <name>{{name}}</name>
          <config>
            <name>{{name}}</name>
          </config>
          <interfaces>
            <interface>
              <id>{{id}}</id>
              <config>
                <id>{{id}}</id>
              </config>
            </interface>
          </interfaces>
        </network-instance>
      </network-instances>
    </config>
  </edit-config>
