  <edit-config>
    <target>
      <{{target}}/>
    </target>
 <default-operation>merge</default-operation>
    <error-option>rollback-on-error</error-option> 
   <config>
      <network-instances xmlns="http://openconfig.net/yang/network-instance">
        <network-instance>
          <name>{{name}}</name>
          <config>
            <name>{{name}}</name>
          </config>
          <protocols>
            <protocol>
              <identifier>{{identifier}}</identifier>
              <name>{{protocol_name}}</name>
              <config>
                <identifier xmlns:oc-pol-types="http://openconfig.net/yang/policy-types">{{identifier}}</identifier>
                <name>{{protocol_name}}</name>
                <enabled>{{enabled}}</enabled>
              </config>
            </protocol>
          </protocols>
        </network-instance>
      </network-instances>
    </config>
  </edit-config>
