  <edit-config>
    <target>
      <{{target}}/>
    </target>
      <config>
      <network-instances xmlns="http://openconfig.net/yang/network-instance">
        <network-instance>
          <name>{{name}}</name>
          <mpls>
            <lsps>
              <constrained-path>
                <tunnels>
                  <tunnel>
                    <name>{{tunnel_name}}</name>
                    <p2p-tunnel-attributes>
                      <p2p-primary-path>
                        <p2p-primary-path>
                          <name>{{p2p_prim_name}}</name>
                          <config>
                            <explicit-path-name>{{explicit_path_name}}</explicit-path-name>
                            <setup-priority>{{setup_priority}}</setup-priority>
                          </config>
                        </p2p-primary-path>
                      </p2p-primary-path>
                    </p2p-tunnel-attributes>
                  </tunnel>
                </tunnels>
              </constrained-path>
            </lsps>
          </mpls>
        </network-instance>
      </network-instances>
    </config>
  </edit-config>
