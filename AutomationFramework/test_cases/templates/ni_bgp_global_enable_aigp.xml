<edit-config>
    <target>
      <{{target}}/>
    </target>
    <config>
      <network-instances xmlns="http://openconfig.net/yang/network-instance">
        <network-instance>
          <name>{{ni_name}}</name>
          <config>
            <name>{{ni_name}}</name>
          </config>
          <protocols>
            <protocol>
              <identifier>{{identifier}}</identifier>
              <name>{{name_protocol}}</name>
              <config>
                <identifier xmlns:oc-pol-types="http://openconfig.net/yang/policy-types">{{identifier}}</identifier>
                <name>{{name_protocol}}</name>
              </config>
              <bgp>
                <global>
                  <config>
                    <as>{{as}}</as>
                  </config>
                  <route-selection-options>
                    <config>
                      <enable-aigp>{{enable_aigp}}</enable-aigp>
                    </config>
                  </route-selection-options>
                </global>
              </bgp>
            </protocol>
          </protocols>
        </network-instance>
      </network-instances>
    </config>
  </edit-config>