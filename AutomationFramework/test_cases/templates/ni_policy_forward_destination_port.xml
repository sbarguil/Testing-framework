  <edit-config>
    <target>
      <{{target}}/>
    </target>
    <config>
       <network-instances xmlns="http://openconfig.net/yang/network-instance">
        <network-instance>
          <name>{{name}}</name>
          <config>
            <name>{{name}}</name>
          </config>
          <policy-forwarding>
            <policies>
              <policy>
                <policy-id>{{policy_id}}</policy-id>
                <config>
                  <policy-id>{{policy_id}}</policy-id>
                </config>
                <rules>
                  <rule>
                    <sequence-id>{{sequence_id}}</sequence-id>
                    <config>
                      <sequence-id>{{sequence_id}}</sequence-id>
                    </config>
                    <transport>
                      <config>
                        <destination-port>{{destination_port}}</destination-port>
                      </config>
                    </transport>
                  </rule>
                </rules>
              </policy>
            </policies>
          </policy-forwarding>
        </network-instance>
      </network-instances>
    </config>
  </edit-config>
