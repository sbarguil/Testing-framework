  <edit-config>
  <target>
      <{{target}}/>
  </target>
  <config>
      <acl xmlns="http://openconfig.net/yang/acl">
        <acl-sets>
          <acl-set>
            <name>{{name}}</name>
            <type>{{type}}</type>
            <acl-entries>
              <acl-entry>
                <sequence-id>{{sequence_id}}</sequence-id>
                <transport>
                  <config>
                    <destination-port>{{dest_port}}</destination-port>
                  </config>
                </transport>
              </acl-entry>
            </acl-entries>
          </acl-set>
        </acl-sets>
      </acl>
    </config>
</edit-config>
