<get>
  <filter>
    <components xmlns="http://openconfig.net/yang/platform">
      <component>
        <name>{{name}}</name>
        <config>
          <name/>
        </config>
        <cpu>
          <utilization xmlns="http://openconfig.net/yang/platform/cpu">
            <state>
              <avg>{{avg}}</avg>
            </state>
          </utilization>
        </cpu>
      </component>
    </components>
  </filter>
</get>
