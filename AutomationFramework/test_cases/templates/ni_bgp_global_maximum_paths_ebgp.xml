<edit-config>
    <target>
      <{{target}}/>
    </target>
    <config>
      <network-instances xmlns="http://openconfig.net/yang/network-instance">
        <network-instance>
          <name>{{ni_name}}</name>
          <config>
            <name>{{ni_name}}</name>
          </config>
          <protocols>
            <protocol>
             <identifier>{{identifier}}</identifier>
              <name>{{protocol_name}}</name>
              <config>
                <identifier xmlns:oc-pol-types="http://openconfig.net/yang/policy-types">{{identifier}}</identifier>
                <name>{{protocol_name}}</name>
              </config>
              <bgp>
                <global>
                  <config>
                     <as>{{as}}</as>
                  </config>
                  <use-multiple-paths>
                    <config>
                      <enabled>{{use_multiple_paths_enabled}}</enabled>
                    </config>
                      <ebgp>
                      <config>
                        <maximum-paths>{{maximum_paths}}</maximum-paths>
                      </config>
                    </ebgp>
                  </use-multiple-paths>
                </global>
              </bgp>
            </protocol>
          </protocols>
        </network-instance>
      </network-instances>
    </config>
  </edit-config>