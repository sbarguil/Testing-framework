  <get>
    <filter>
      <network-instances xmlns="http://openconfig.net/yang/network-instance">
        <network-instance>
          <name>{{name}}</name>
          <mpls>
            <signaling-protocols>
              <rsvp-te>
                <sessions>
                  <session>
                    <local-index>{{local_index}}</local-index>
                    <state>
                      <local-index>{{local_index}}</local-index>
                    </state>
                  </session>
                </sessions>
              </rsvp-te>
            </signaling-protocols>
          </mpls>
        </network-instance>
      </network-instances>
    </filter>
  </get>