<get>
  <filter>
      <network-instances xmlns="http://openconfig.net/yang/network-instance">
        <network-instance>
          <protocols>
            <protocol>
              <igmp>
                <interfaces>
                  <interface>
                    <state>
                      <enabled>{{enabled}}</enabled>
                    </state>
                  </interface>
                </interfaces>
              </igmp>
            </protocol>
          </protocols>
        </network-instance>
      </network-instances>
    </filter>
</get>
