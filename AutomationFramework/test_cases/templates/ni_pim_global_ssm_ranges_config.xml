  <edit-config>
    <target>
      <{{target}}/>
    </target>
    <config>
      <network-instances xmlns="http://openconfig.net/yang/network-instance">
        <network-instance>
          <name>{{name}}</name>
          <config>
            <name>{{name}}</name>
          </config>
          <protocols>
            <protocol>
              <pim>
                <global>
                  <ssm>
                    <config>
                      <ssm-ranges>{{ssm_ranges}}</ssm-ranges>
                    </config>
                  </ssm>
                </global>
              </pim>
            </protocol>
          </protocols>
        </network-instance>
      </network-instances>
    </config>
  </edit-config>
