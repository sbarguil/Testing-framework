<edit-config>
     <target>
      <{{target}}/>
    </target>
    <config>
      <network-instances xmlns="http://openconfig.net/yang/network-instance">
        <network-instance>
          <name>{{ni_name}}</name>
          <config>
            <name>{{ni_name}}</name>
          </config>
          <protocols>
            <protocol>
              <identifier>{{identifier}}</identifier>
              <name>{{name_protocol}}</name>
              <config>
                <identifier xmlns:oc-pol-types="http://openconfig.net/yang/policy-types">{{identifier}}</identifier>
                <name>{{name_protocol}}</name>
              </config>
              <bgp>
                <global>
                  <config>
                    <as>{{as}}</as>
                  </config>
                  <afi-safis>
                    <afi-safi>
                      <afi-safi-name>{{afi_safi_name}}</afi-safi-name>
                      <config>
                        <afi-safi-name xmlns:oc-bgp-types="http://openconfig.net/yang/bgp-types">{{afi_safi_name}}</afi-safi-name>
                        <enabled>{{enabled}}</enabled>
                      </config>
                    </afi-safi>
                  </afi-safis>
                </global>
              </bgp>
            </protocol>
          </protocols>
        </network-instance>
      </network-instances>
    </config>
  </edit-config>