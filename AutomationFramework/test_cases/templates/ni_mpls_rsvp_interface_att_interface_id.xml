 <edit-config>
    <target>
      <{{target}}/>
    </target>
     <config>
      <network-instances xmlns="http://openconfig.net/yang/network-instance">
        <network-instance>
          <name>{{name}}</name>
          <config>
            <name>{{name}}</name>
            <type xmlns:oc-ni-types="http://openconfig.net/yang/network-instance-types">{{type}}</type>
            <enabled>{{enable}}</enabled>
            <description>{{description}}</description>
          </config>    
            <signaling-protocols>
              <rsvp-te>
                <interface-attributes>
                  <interface>
                    <interface-id>{{id}}</interface-id>
                    <config>
                      <interface-id>{{id}}</interface-id>
                    </config>
                  </interface>
                </interface-attributes>
              </rsvp-te>
            </signaling-protocols>
          </mpls>
        </network-instance>
      </network-instances>
    </config>
  </edit-config>
