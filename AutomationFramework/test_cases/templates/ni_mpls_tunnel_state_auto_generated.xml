<get>
    <filter>
      <network-instances xmlns="http://openconfig.net/yang/network-instance">
        <network-instance>
          <name>{{ni_name}}</name>
          <mpls>
            <lsps>
              <constrained-path>
                <tunnels>
                  <tunnel>
                    <name>{{tunnel_name}}</name>
                    <state>
                      <auto-generated>{{auto_generated}}</auto-generated>
                    </state>
                  </tunnel>
                </tunnels>
              </constrained-path>
            </lsps>
          </mpls>
        </network-instance>
      </network-instances>
    </filter>
  </get>

