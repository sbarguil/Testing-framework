  <get>
    <filter>
      <network-instances xmlns="http://openconfig.net/yang/network-instance">
         <network-instance>
          <name>{{name}}</name>
          <config>
            <name>{{name}}</name>
            <type xmlns:oc-ni-types="http://openconfig.net/yang/network-instance-types">{{type}}</type>
            <enabled>{{enable}}</enabled>
            <description>{{description}}</description>
          </config>   
          <mpls>
            <lsps>
              <constrained-path>
                <tunnels>
                  <tunnel>
                     <name>{{tunnel_name}}</name>
                    <config>
                     <name>{{tunnel_name}}</name>
                    </config>
                    <p2p-tunnel-attributes>
                      <p2p-primary-path>
                        <p2p-primary-path>
                          <name>{{p2p_prim_path_name}}</name>
                          <state>
                            <name>{{p2p_prim_path_name}}</name>
                            <associated-rsvp-sessions>{{associated_rsvp_sessions}}</associated-rsvp-sessions>
                          </state>
                        </p2p-primary-path>
                      </p2p-primary-path>
                    </p2p-tunnel-attributes>
                  </tunnel>
                </tunnels>
              </constrained-path>
            </lsps>
          </mpls>
        </network-instance>
      </network-instances>
    </filter>
  </get>