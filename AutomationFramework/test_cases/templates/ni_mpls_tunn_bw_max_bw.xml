  <edit-config>
    <target>
      <{{target}}/>
    </target>
     <config>
      <network-instances xmlns="http://openconfig.net/yang/network-instance">
        <network-instance>
          <name>{{name}}</name>
          <mpls>
            <lsps>
              <constrained-path>
                <tunnels>
                  <tunnel>
                    <name>{{tunnel_name}}</name>
                    <bandwidth>
                      <auto-bandwidth>
                        <config>
                          <max-bw>{{max_bw}}</max-bw>
                        </config>
                      </auto-bandwidth>
                    </bandwidth>
                  </tunnel>
                </tunnels>
              </constrained-path>
            </lsps>
          </mpls>
        </network-instance>
      </network-instances>
    </config>
  </edit-config>
