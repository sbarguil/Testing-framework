  <edit-config>
    <target>
      <{{target}}/>
    </target>
   <config>
      <network-instances xmlns="http://openconfig.net/yang/network-instance">
         <network-instance>
          <name>{{name}}</name>
          <config>
            <name>{{name}}</name>
            <type xmlns:oc-ni-types="http://openconfig.net/yang/network-instance-types">{{type}}</type>
            <enabled>{{enable}}</enabled>
            <description>{{description}}</description>
          </config>   
          <mpls>
            <lsps>
              <constrained-path>
                <tunnels>
                  <tunnel>
                   <name>{{tunnel_name}}</name>
                    <config>
                     <name>{{tunnel_name}}</name>
                    </config>
                    <p2p-tunnel-attributes>
                      <p2p-primary-path>
                        <p2p-primary-path>
                          <name>{{p2p_prim_name}}</name>
                          <config>
                            <path-computation-server>{{ip_server}}</path-computation-server>
                            <name>{{p2p_prim_name}}</name>
                          </config>
                        </p2p-primary-path>
                      </p2p-primary-path>
                    </p2p-tunnel-attributes>
                  </tunnel>
                </tunnels>
              </constrained-path>
            </lsps>
          </mpls>
        </network-instance>
      </network-instances>
    </config>
  </edit-config>
