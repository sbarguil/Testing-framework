 <get>
    <filter>
      <network-instances xmlns="http://openconfig.net/yang/network-instance">
        <network-instance>
         <name>{{name}}</name>
          <config>
            <name>{{name}}</name>
            <type xmlns:oc-ni-types="http://openconfig.net/yang/network-instance-types">{{type}}</type>
            <enabled>{{enable}}</enabled>
            <description>{{description}}</description>
          </config>
          <mpls>
            <lsps>
              <constrained-path>
                <tunnels>
                  <tunnel>
                     <name>{{tunnel_name}}</name>
                    <config>
                      <name>{{tunnel_name}}</name>
                    </config>
                    <bandwidth>
                      <state>
                        <signaled-bandwidth>{{signaled_bw}}<signaled-bandwidth>
                      </state>
                    </bandwidth>
                  </tunnel>
                </tunnels>
              </constrained-path>
            </lsps>
          </mpls>
        </network-instance>
      </network-instances>
    </filter>
  </get>